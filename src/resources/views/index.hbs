    <!-- Women Banner Section Begin -->
    <section class="women-banner spad">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-3">
                    <div class="product-large set-bg" data-setbg="img/create-img/ronaldo-and-messi.jpg">
                        <h2>La Liga</h2>
                        <a href="/product">Discover More</a>
                    </div>
                </div>
                <div class="col-lg-8 offset-lg-1">
                    <div class="filter-control">
                        <ul>
                            <li class="active">Clothings</li>
                            <li>HandBag</li>
                            <li>Shoes</li>
                            <li>Accessories</li>
                        </ul>
                    </div>
                    <div class="product-slider owl-carousel">
                        {{#each laLiga}}
                        <div class="product-item">
                            <div class="pi-pic">
                                <img src="{{this.image}}" alt="">
                                <div class="sale">Sale</div>
                                <div class="icon">
                                    <i class="icon_heart_alt"></i>
                                </div>
                                <ul>
                                    <li class="w-icon active"><a href="/product/add-to-cart/{{this._id}}"><i class="icon_bag_alt"></i></a></li>
                                    <li class="quick-view"><a href="/product/{{this.slug}}">Quick View</a></li>
                                </ul>
                            </div>
                            <div class="pi-text">
                                <div class="catagory-name">Coat</div>
                                <a href="#">
                                    <h5>{{this.name}}</h5>
                                </a>
                                <div class="product-price">
                                    {{this.price}}.000đ                                
                                </div>
                            </div>
                        </div>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Women Banner Section End -->

